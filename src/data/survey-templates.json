    {
      "id": "customer-satisfaction",
      "name": "Customer Satisfaction Survey",
      "description": "Measure customer satisfaction with your product or service",
      "category": "Business",
      "icon": "üë•",
      "questions": [
        {
          "id": "overall-satisfaction",
          "type": "rating",
          "question": "How satisfied are you with our product/service overall?",
          "required": true,
          "options": {
            "scale": 5,
            "labels": ["Very Dissatisfied", "Dissatisfied", "Neutral", "Satisfied", "Very Satisfied"]
          }
        },
        {
          "id": "recommendation",
          "type": "nps",
          "question": "How likely are you to recommend us to a friend or colleague?",
          "required": true
        },
        {
          "id": "improvement-areas",
          "type": "checkbox",
          "question": "Which areas do you think we could improve? (Select all that apply)",
          "required": false,
          "options": {
            "choices": [
              "Product Quality",
              "Customer Service",
              "Pricing",
              "Delivery Speed",
              "Website Experience",
              "Communication"
            ]
          }
        },
        {
          "id": "additional-feedback",
          "type": "textarea",
          "question": "Please share any additional feedback or suggestions:",
          "required": false,
          "options": {
            "placeholder": "Your feedback helps us improve..."
          }
        }
      ],
      "settings": {
        "allowAnonymous": true,
        "requireAuth": false,
        "multipleResponses": false,
        "showProgressBar": true,
        "thankYouMessage": "Thank you for your valuable feedback!"
      }
    },
